{
  "version": 2,
  "outputDirectory": "public",
  "functions": {
    "api/simple.cjs": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api/simple.cjs"
    },
    {
      "src": "/api/health",
      "dest": "/api/simple.cjs"
    },
    {
      "src": "/",
      "dest": "/index_with_auth.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index_with_auth.html"
    }
  ]
}
