{
  "version": 2,
  "outputDirectory": "public",
  "functions": {
    "api/index.cjs": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/jobs",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/jobs/(.*)",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/health",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/test-supabase",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/test-create-points",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/test-upload",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/",
      "dest": "/index_with_auth.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index_with_auth.html"
    }
  ]
}
